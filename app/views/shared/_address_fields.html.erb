<div class="row address">
  <div class="col-lg-12 address-lines">
    <%= f.input :line1, label: 'Line 1' %>
    <%= f.input :line2, label: 'Line 2' %>
  </div>
  <div class="col-lg-7 address-city">
    <%= f.input :city %>
  </div>
  <div class="col-lg-2 address-state">
    <%= f.input :state %>
  </div>
  <div class="col-lg-3 address-zip">
    <%= f.input :zip %>
  </div>
</div>